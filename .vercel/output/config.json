{
  "version": 3,
  "routes": [
    {
      "src": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$",
      "headers": {
        "Location": "/$1"
      },
      "status": 308,
      "continue": true
    },
    {
      "src": "/_next/__private/trace",
      "dest": "/404",
      "status": 404,
      "continue": true
    },
    {
      "src": "/404/?",
      "status": 404,
      "continue": true,
      "missing": [
        {
          "type": "header",
          "key": "x-prerender-revalidate"
        }
      ]
    },
    {
      "src": "/500",
      "status": 500,
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/_next/data/(.*)",
      "dest": "/_next/data/$1",
      "check": true
    },
    {
      "handle": "resource"
    },
    {
      "src": "/.*",
      "status": 404
    },
    {
      "handle": "miss"
    },
    {
      "src": "/_next/static/(?:[^/]+/pages|pages|chunks|runtime|css|image|media)/.+",
      "status": 404,
      "check": true,
      "dest": "$0"
    },
    {
      "handle": "rewrite"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/index.json$",
      "dest": "/"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/auth.json$",
      "dest": "/auth"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/authors/(?<author_id>[^/]+?)\\.json$",
      "dest": "/authors/[author_id]?author_id=$author_id"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/authors/(?<author_id>[^/]+?)/edit\\.json$",
      "dest": "/authors/[author_id]/edit?author_id=$author_id"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/authors/(?<author_id>[^/]+?)/posts/create\\.json$",
      "dest": "/authors/[author_id]/posts/create?author_id=$author_id"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/authors/(?<author_id>[^/]+?)/posts/(?<post_id>[^/]+?)\\.json$",
      "dest": "/authors/[author_id]/posts/[post_id]?author_id=$author_id&post_id=$post_id"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/authors/(?<author_id>[^/]+?)/posts/(?<post_id>[^/]+?)/edit\\.json$",
      "dest": "/authors/[author_id]/posts/[post_id]/edit?author_id=$author_id&post_id=$post_id"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/onboarding.json$",
      "dest": "/onboarding"
    },
    {
      "src": "^/_next/data/8xRkPOy3qaaBHQ2let9Ry/search.json$",
      "dest": "/search"
    },
    {
      "src": "/_next/data/(.*)",
      "dest": "/404",
      "status": 404
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)(?:/)?$",
      "dest": "/api/authors/[author_id]?author_id=$author_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/followers(?:/)?$",
      "dest": "/api/authors/[author_id]/followers?author_id=$author_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/followers/(?<foreign_author_id>[^/]+?)(?:/)?$",
      "dest": "/api/authors/[author_id]/followers/[foreign_author_id]?author_id=$author_id&foreign_author_id=$foreign_author_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/inbox(?:/)?$",
      "dest": "/api/authors/[author_id]/inbox?author_id=$author_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/liked(?:/)?$",
      "dest": "/api/authors/[author_id]/liked?author_id=$author_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/posts(?:/)?$",
      "dest": "/api/authors/[author_id]/posts?author_id=$author_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/posts/(?<post_id>[^/]+?)(?:/)?$",
      "dest": "/api/authors/[author_id]/posts/[post_id]?author_id=$author_id&post_id=$post_id"
    },
    {
      "src": "^/api/authors/(?<author_id>[^/]+?)/posts/(?<post_id>[^/]+?)/comments(?:/)?$",
      "dest": "/api/authors/[author_id]/posts/[post_id]/comments?author_id=$author_id&post_id=$post_id"
    },
    {
      "src": "^/authors/(?<author_id>[^/]+?)(?:/)?$",
      "dest": "/authors/[author_id]?author_id=$author_id"
    },
    {
      "src": "^/authors/(?<author_id>[^/]+?)/edit(?:/)?$",
      "dest": "/authors/[author_id]/edit?author_id=$author_id"
    },
    {
      "src": "^/authors/(?<author_id>[^/]+?)/posts/create(?:/)?$",
      "dest": "/authors/[author_id]/posts/create?author_id=$author_id"
    },
    {
      "src": "^/authors/(?<author_id>[^/]+?)/posts/(?<post_id>[^/]+?)(?:/)?$",
      "dest": "/authors/[author_id]/posts/[post_id]?author_id=$author_id&post_id=$post_id"
    },
    {
      "src": "^/authors/(?<author_id>[^/]+?)/posts/(?<post_id>[^/]+?)/edit(?:/)?$",
      "dest": "/authors/[author_id]/posts/[post_id]/edit?author_id=$author_id&post_id=$post_id"
    },
    {
      "handle": "hit"
    },
    {
      "src": "/_next/static/(?:[^/]+/pages|pages|chunks|runtime|css|image|media|8xRkPOy3qaaBHQ2let9Ry)/.+",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true,
      "important": true
    },
    {
      "src": "/index",
      "headers": {
        "x-matched-path": "/"
      },
      "continue": true,
      "important": true
    },
    {
      "src": "/((?!index$).*)",
      "headers": {
        "x-matched-path": "/$1"
      },
      "continue": true,
      "important": true
    },
    {
      "handle": "error"
    },
    {
      "src": "/.*",
      "dest": "/404",
      "status": 404
    },
    {
      "src": "/.*",
      "dest": "/500",
      "status": 500
    }
  ],
  "images": {
    "domains": [
      "*"
    ],
    "sizes": [
      640,
      750,
      828,
      1080,
      1200,
      1920,
      2048,
      3840,
      16,
      32,
      48,
      64,
      96,
      128,
      256,
      384
    ],
    "remotePatterns": [],
    "minimumCacheTTL": 60,
    "formats": [
      "image/webp"
    ],
    "dangerouslyAllowSVG": false,
    "contentSecurityPolicy": "script-src 'none'; frame-src 'none'; sandbox;"
  },
  "overrides": {
    "404.html": {
      "path": "404",
      "contentType": "text/html; charset=utf-8"
    },
    "500.html": {
      "path": "500",
      "contentType": "text/html; charset=utf-8"
    }
  },
  "framework": {
    "version": "13.1.6"
  },
  "crons": []
}
